<style>
  h6 { cursor: pointer; }
</style>

<script>
  $(document).on('click', 'h6', function(){
    $(this).next().toggle();
  })
</script>

<h3>Last 100 webhook recieved events we received from Flow</h3>

<br />

<% for title, data in @events %>
  <h6><%= title %></h6>
  <pre><%= JSON.pretty_generate(data) %></pre>
  <hr />
<% end %>